{
  "cli": {
    "version": ">= 3.10.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://healthdb-backend.onrender.com",
        "EXPO_PUBLIC_API_KEY": "qwerty12345"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://healthdb.cyclic.cloud",
        "EXPO_PUBLIC_API_KEY": "qwerty12345",
        "EXPO_PUBLIC_HIDDEN_FEATURES": "true"
      }
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://healthdb.cyclic.cloud",
        "EXPO_PUBLIC_API_KEY": "qwerty12345",
        "EXPO_PUBLIC_HIDDEN_FEATURES": "true"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
